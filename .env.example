# Keys for connection to the Ethereum network through Infura
INFURA_KEY=your_infura_project_key

# Private key of the wallet to be used for the deployment of the smart contract
PRIVATE_KEY=your_wallet_private_key_without_0x_prefix

# WalletConnect/Reown project ID for wallet connection functionality
NEXT_PUBLIC_PROJECT_ID=your_project_id

# IPFS configuration for NFT metadata
NEXT_PUBLIC_IPFS_GATEWAY=https://your-ipfs-gateway.example.com

# Address of the Smart Contract Moovenft
NEXT_PUBLIC_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000